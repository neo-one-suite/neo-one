{
  "name": "neo-one",
  "author": "Alex DiCarlo <alexdicarlo@gmail.com>",
  "version": "0.0.0",
  "description": "NEOâ€¢ONE is a suite of tools and frameworks for developing dapps on the NEO blockchain.",
  "homepage": "https://neo-one.io",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/neo-one-suite/neo-one.git"
  },
  "bugs": {
    "url": "https://github.com/neo-one-suite/neo-one/issues"
  },
  "license": "MIT",
  "keywords": [
    "neo",
    "blockchain",
    "neo-one",
    "dapp"
  ],
  "private": true,
  "workspaces": [
    "packages/*"
  ],
  "engines": {
    "node": ">=10.6.0"
  },
  "lint-staged": {
    "packages/*/src/**/*.ts": [
      "yarn run prettier --write --config .prettierrc",
      "git add",
      "yarn test --findRelatedTests --ci --passWithNoTests -w 4",
      "yarn run lint-files"
    ],
    "scripts/**/*.js": [
      "yarn run prettier --write --config .prettierrc --parser flow",
      "git add"
    ],
    "scripts/**/*.ts": [
      "yarn run prettier --write --config .prettierrc",
      "git add",
      "yarn run lint-files"
    ]
  },
  "scripts": {
    "lint-staged": "node ./node_modules/.bin/lint-staged",
    "gulp": "cross-env NODE_OPTIONS=\"--max-old-space-size=8192\" gulp",
    "build": "yarn run gulp build",
    "build:e2e": "yarn run gulp buildE2E",
    "watch": "yarn run gulp watch",
    "release": "yarn run gulp release",
    "neo-one": "node ./dist/neo-one/packages/neo-one-cli/bin/neo-one",
    "test": "jest --config jest/unit.js",
    "test-ci": "jest --config jest/unit-ci.js --ci -w 2",
    "e2e": "jest --config jest/e2e.js",
    "e2e-ci": "jest --config jest/e2e-ci.js --ci -w 1",
    "report-coverage": "yarn run nyc report --report=json && yarn run codecov -f coverage/*.json",
    "tsc": "tsc",
    "ts-node": "cross-env NODE_OPTIONS=\"--max-old-space-size=8192\" TS_NODE_PROJECT=tsconfig/tsconfig.es2018.cjs.json node -r ts-node/register/transpile-only",
    "nit:source": "prettier --config .prettierrc --list-different \"packages/*/src/**/*.ts\"",
    "nit:scripts:ts": "prettier --config .prettierrc --list-different \"scripts/**/*.ts\"",
    "nit:scripts:js": "prettier --config .prettierrc --list-different \"scripts/**/*.js\" --parser flow",
    "nit:scripts": "yarn run nit:scripts:ts && yarn run nit:scripts:js",
    "nit": "yarn run nit:source && yarn run nit:scripts",
    "prettier:source": "prettier --config .prettierrc --write \"packages/*/src/**/*.ts\"",
    "prettier:scripts:ts": "prettier --config .prettierrc --write \"scripts/**/*.ts\"",
    "prettier:scripts:js": "prettier --config .prettierrc --write \"scripts/**/*.js\" --parser flow",
    "prettier:scripts": "yarn run prettier:scripts:ts && yarn run prettier:scripts:js",
    "prettier:all": "yarn run prettier:source && yarn run prettier:scripts",
    "lint-files": "tslint --project . --format codeFrame",
    "lint:ts": "yarn run lint-files 'packages/*/src/**/*.ts'",
    "lint:scripts": "yarn run lint-files 'scripts/**/*.ts'",
    "lint": "yarn run lint:ts && yarn run lint:scripts",
    "verify-storage": "yarn run ts-node ./scripts/verify/storage.ts",
    "generate-sc-types": "yarn run ts-node ./packages/neo-one-smart-contract-compiler/src/scripts/generateTypes.ts",
    "create-guards": "yarn run ts-node ./scripts/ts/createGuards.ts",
    "contributors:add": "all-contributors add",
    "contributors:generate": "all-contributors generate"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged && echo \"Executing tsc...\" && tsc",
      "post-checkout": "cross-env-shell \"yarn run ts-node ./scripts/hooks/post-checkout.ts $HUSKY_GIT_PARAMS\"",
      "post-merge": "yarn run ts-node ./scripts/hooks/post-merge.ts",
      "post-rewrite": "yarn run ts-node ./scripts/hooks/post-rewrite.ts"
    }
  },
  "nyc": {
    "reporter": ["lcov", "text"],
    "tempDirectory": "./coverage"
  },
  "devDependencies": {
    "@babel/core": "7.0.0-beta.55",
    "@babel/plugin-syntax-dynamic-import": "7.0.0-beta.55",
    "@babel/plugin-syntax-numeric-separator": "7.0.0-beta.55",
    "@babel/plugin-syntax-optional-catch-binding": "7.0.0-beta.55",
    "@babel/plugin-syntax-typescript": "7.0.0-beta.55",
    "@reactivex/ix-esnext-cjs": "2.3.5",
    "@reactivex/ix-esnext-esm": "2.3.5",
    "@types/babel__code-frame": "7.0.0",
    "@types/bs58": "3.0.30",
    "@types/chokidar": "1.7.5",
    "@types/cli-table2": "0.2.1",
    "@types/core-js": "2.5.0",
    "@types/execa": "0.9.0",
    "@types/figures": "2.0.0",
    "@types/fs-extra": "5.0.4",
